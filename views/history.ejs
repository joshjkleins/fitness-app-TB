<%- include('partials/header') %>
<link rel="stylesheet" href="/css/history.css">

<h1>History Page</h1>
<a href="/">Home Page</a>
<br>
<br>
<h3>Month</h3>
<div class="wrapper">
    <header>
        <p class="current-date"></p>
        <div class="icons">
        <span id="prev" class="material-symbols-rounded">chevron_left</span>
        <span id="next" class="material-symbols-rounded">chevron_right</span>
        </div>
    </header>
    <div class="calendar">
        <ul class="weeks">
        <li>Sun</li>
        <li>Mon</li>
        <li>Tue</li>
        <li>Wed</li>
        <li>Thu</li>
        <li>Fri</li>
        <li>Sat</li>
        </ul>
        <ul class="days"></ul>
    </div>
</div>
<br>
<br>
<table>
    <tr>
        <td class="days-of-week" colspan="2">Sunday</td>
        <td class="days-of-week" colspan="2">Monday</td>
        <td class="days-of-week" colspan="2">Tuesday</td>
        <td class="days-of-week" colspan="2">Wednesday</td>
        <td class="days-of-week" colspan="2">Thursday</td>
        <td class="days-of-week" colspan="2">Friday</td>
        <td class="days-of-week" colspan="2">Saturday</td>
    </tr>
    <tr class="dates">
        <td class="shruken-cell">1</td>
        <td></td>
        <td>2</td>
        <td></td>
        <td>3</td>
        <td></td>
        <td>4</td>
        <td></td>
        <td>5</td>
        <td></td>
        <td>6</td>
        <td></td>
        <td>7</td>
        <td></td>
    </tr>
    <tr class="data">
        <td colspan="2"></td>
        <td colspan="2"></td>
        <td colspan="2"></td>
        <td colspan="2"></td>
        <td colspan="2"></td>
        <td colspan="2"></td>
        <td colspan="2"></td>
    </tr>
    

</table>
<script src="/js/history.js" defer></script>

<%- include('partials/footer') %>